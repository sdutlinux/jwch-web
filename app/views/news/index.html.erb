<h1>Newss</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>序号</th>
      <th>标题</th>
      <th>创建时间</th>
    </tr>
  </thead>
  <tbody>
    <% @news.each do |news| %>
      <tr>
        <td><%= news.id %></td>
        <td><%= link_to news.title, show_news_path(news) %></td>
        <td><%= news.created_at %></td>
        <td>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

